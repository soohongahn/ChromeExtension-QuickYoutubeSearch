{
  "name": "Quick Youtube Search",
  "description": "Type 'yt' space a keyword to search on youtube.",
  "version": "1.0",
  "manifest_version": 3,

  "background": {
    "service_worker": "search.js"
  },

  "omnibox": { "keyword" : "yt" },

  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
